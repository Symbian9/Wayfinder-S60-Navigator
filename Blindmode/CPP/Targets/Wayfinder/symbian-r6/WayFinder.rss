// ---------------------------------------------------------
//
// r_sockets_dialog_connect
// Dialog for connection details
//
// ---------------------------------------------------------

RESOURCE DIALOG r_sockets_dialog_connect
{
    title = "Connect details";
    buttons = r_wayfinder_softkeys_ok_cancel;
    flags = EEikDialogFlagNoDrag | 
            EEikDialogFlagFillAppClientRect |
            EEikDialogFlagCbaButtons |
            EEikDialogFlagWait;

    items =
    {
        DLG_LINE
        {
			type = EEikCtEdwin;
			prompt = "Server name";
			id = ESocketsServerNameEdwin;
			control = EDWIN
			{
				width = 10;
                maxlength = EDWIN_MAXLENGTH;
			};
        },
        DLG_LINE
        {
			type = EEikCtNumberEditor;
			prompt = "Port number";
			id = ESocketsPortNumberEdwin;
			control = NUMBER_EDITOR
			{
                min = 0;
				max = 65534;
			};
        }
    };
}


